<%# views/telemetry.ejs %>
 <% extend('layout') %>

<h1>Telemetry</h1>
<div class="table-responsive">
	<table class="table table-striped" id="telemetry-data">
		<thead>
			<tr>
				<th>Time</th>
				<th>P</th>
				<th>Vrms</th>
				<th>Irms</th>
				<th>Dev. Id</th>
			</tr>
		</thead>
		<tbody>
			<% telemetry_list.forEach(function(item) { %>
		    <tr>
		    	<td><%= item.timestamp %></td>
		    	<td><%= item.power %></td>
		    	<td><%= item.voltage %></td>
		    	<td><%= item.current %></td>
		    	<td><%= item.deviceId %></td>
		    </tr>
		    <% }); %>
		</tbody>
	</table>
</div> 

